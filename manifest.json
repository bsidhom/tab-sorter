{
  "manifest_version": 3,
  "name": "Tab Sorter",
  "version": "1.0.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_title": "Sort Tabs"
  },
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
